# Leosac WebSocket server address (single)
LEOSAC_ADDR=ws://localhost:8888

# Optional: Multiple Leosac WebSocket server addresses (comma-separated)
# Example: ws://server-a:8888, ws://server-b:8888
LEOSAC_ADDRS=

# Flask secret key (change in production)
SECRET_KEY=your-secret-key-here

# Flask debug mode
FLASK_DEBUG=True 